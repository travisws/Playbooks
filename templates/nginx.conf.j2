worker_processes 1;
events { worker_connections 1024; }
http {
    server {
        listen {{ nginx_listen_port }};
        root {{ nginx_document_root }};
        location / {
            try_files $uri.html $uri =404;
            index index.html;
        }
    }
    error_log /var/log/nginx/error.log;
    access_log /var/log/nginx/access.log;
}
